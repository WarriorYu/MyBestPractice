<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/aop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AOP Demo" />

    <Button
        android:id="@+id/service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Service Demo" />

    <Button
        android:id="@+id/custom_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="service"
        android:text="Custom View Demo" />

    <Button
        android:id="@+id/kotlin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="service"
        android:text="Kotlin Demo" />


    <Button
        android:id="@+id/optimize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="service"
        android:text="Optimize Demo" />

    <Button
        android:id="@+id/mvvm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="service"
        android:text="MVVM Demo" />

    <Button
        android:id="@+id/Rxjava3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rxjava3源码解析" />
</LinearLayout>